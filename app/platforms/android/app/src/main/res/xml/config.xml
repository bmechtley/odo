<?xml version='1.0' encoding='utf-8'?>
<widget id="edu.asu.ame.odo" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="OSC">
        <param name="android-package" value="nl.sylvain.cordova.osc.OSCUtils" />
    </feature>
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="SpeechRecognition">
        <param name="android-package" value="com.pbakondy.SpeechRecognition" />
    </feature>
    <feature name="Notification">
        <param name="android-package" value="org.apache.cordova.dialogs.Notification" />
    </feature>
    <feature name="WifiManagerPlugin">
        <param name="android-package" value="dk.kapetanovic.wifimanager.WifiManagerPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="OdoSensors">
        <param name="android-package" value="edu.asu.ame.plugin.OdoSensors" />
    </feature>
    <feature name="BLE">
        <param name="android-package" value="com.megster.cordova.ble.central.BLECentralPlugin" />
    </feature>
    <name>Odo</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="android-minSdkVersion" value="23" />
</widget>
